@page
@model MyApp.Namespace.IndexModel
@{
}

<div class="container">
    <h1>Listado de Familias</h1>
    <a asp-page="Create" class="btn btn-primary">Crear Nuevo</a>
    <hr>
    <table class="table">
        <thead>
            <tr>
                <th>Id</thd>
                <th>Nombre</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var i in Model.Familias){
            <tr>
                <td>@i.Id</td>
                <td>@i.Nombre</td>
            </tr>
            }
        </tbody>
    </table>
</div>
